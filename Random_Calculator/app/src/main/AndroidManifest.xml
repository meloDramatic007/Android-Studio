<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.saddan.random_calculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MinMax"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Averege"
            android:parentActivityName=".MainActivity" ></activity>
        <activity android:name=".Calculator"
            android:parentActivityName=".MainActivity" ></activity>
        <activity android:name=".PrimeNumber"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Odd_Even"
            android:parentActivityName=".MainActivity" ></activity>
        <activity android:name=".StringLength"
            android:parentActivityName=".MainActivity" ></activity>
        <activity android:name=".Pallindrome"
            android:parentActivityName=".MainActivity" ></activity>
    </application>

</manifest>